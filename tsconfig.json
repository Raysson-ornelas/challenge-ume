{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/utils/calculateMoveRobot.js", "pages/contexts/gameContext.js", "pages/components/squares.js", "pages/components/controlls.js", "pages/components/board.js", "pages/index.js", "pages/_app.js"],
  "exclude": ["node_modules"]
}
